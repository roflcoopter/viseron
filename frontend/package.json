  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "e2e:update-screenshots": "playwright test e2e/tests/docs.spec.ts -c e2e/playwright.config.ts",
    "lint:eslint": "eslint \"src/**/**.{js,jsx,ts,tsx,json}\"",
    "format:eslint": "eslint \"src/**/**.{js,jsx,ts,tsx,json}\" --fix",
    "lint:prettier": "prettier \"src/**/**.{js,jsx,ts,tsx,json}\" --check",
    "format:prettier": "prettier \"src/**/**.{js,jsx,ts,tsx,json}\" --write",
    "format": "npm run format:prettier && npm run format:eslint",
    "lint:types": "tsc --pretty --noEmit",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:types",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "overrides": {
    "react-page-visibility": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-lazyload": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@jy95/material-ui-image": {
      "react": "$react",
      "react-dom": "$react-dom",
      "@mui/material": "$@mui/material",
      "@mui/icons-material": "$@mui/icons-material"
    },
    "mui-nested-menu": {
      "@mui/material": "$@mui/material",
      "@mui/icons-material": "$@mui/icons-material"
    }
  }
}
